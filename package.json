{
  "name": "streamnook",
  "version": "3.1.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "tauri": "tauri",
    "version:patch": "node scripts/increment-version.js patch",
    "version:minor": "node scripts/increment-version.js minor",
    "version:major": "node scripts/increment-version.js major",
    "version:patch:auto": "node scripts/increment-version.js patch --commit --tag --push",
    "version:minor:auto": "node scripts/increment-version.js minor --commit --tag --push",
    "version:major:auto": "node scripts/increment-version.js major --commit --tag --push",
    "release:patch": "npm run version:patch && npm run tauri build",
    "release:minor": "npm run version:minor && npm run tauri build",
    "release:major": "npm run version:major && npm run tauri build",
    "release:patch:auto": "npm run version:patch:auto && npm run tauri build",
    "release:minor:auto": "npm run version:minor:auto && npm run tauri build",
    "release:major:auto": "npm run version:major:auto && npm run tauri build",
    "ship": "powershell -ExecutionPolicy Bypass -File ./scripts/release_manager.ps1"
  },
  "dependencies": {
    "@tauri-apps/api": "^2.9.0",
    "@tauri-apps/plugin-shell": "^2.3.3",
    "autoprefixer": "^10.4.22",
    "framer-motion": "^12.23.24",
    "hls.js": "^1.5.17",
    "lucide-react": "^0.553.0",
    "plyr": "^3.7.8",
    "postcss": "^8.5.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-virtualized-auto-sizer": "^1.0.26",
    "react-window": "^1.8.11",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^2.9.4",
    "@types/node": "^24.10.1",
    "@types/react": "^18.3.26",
    "@types/react-dom": "^18.3.7",
    "@types/react-window": "^1.8.8",
    "@vitejs/plugin-react": "^4.7.0",
    "tailwindcss": "^3.4.18",
    "typescript": "^5.6.3",
    "vite": "^5.4.21"
  }
}




