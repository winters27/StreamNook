{
  "name": "streamnook",
  "version": "4.8.4",
  "private": true,
  "scripts": {
    "dev": "vite",
    "devtools": "react-devtools",
    "dev:debug": "concurrently \"npm run devtools\" \"npm run tauri dev\" --names \"DevTools,Tauri\" --prefix-colors \"blue,green\"",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "tauri": "tauri",
    "ship": "powershell -ExecutionPolicy Bypass -File ./scripts/release_manager.ps1",
    "update-deps": "powershell -ExecutionPolicy Bypass -File ./scripts/update-dependencies.ps1",
    "update-deps:auto": "powershell -ExecutionPolicy Bypass -File ./scripts/update-dependencies.ps1 -Auto"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.87.0",
    "@tauri-apps/api": "^2.9.0",
    "@tauri-apps/plugin-dialog": "^2.2.0",
    "@tauri-apps/plugin-notification": "^2.3.3",
    "@tauri-apps/plugin-shell": "^2.3.3",
    "autoprefixer": "^10.4.22",
    "framer-motion": "^12.23.25",
    "hls.js": "^1.5.17",
    "lucide-react": "^0.556.0",
    "phosphor-react": "^1.4.1",
    "plyr": "^3.7.8",
    "postcss": "^8.5.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-virtualized-auto-sizer": "^1.0.26",
    "react-window": "^1.8.11",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^2.9.5",
    "@types/node": "^24.10.1",
    "@types/react": "^18.3.26",
    "@types/react-dom": "^18.3.7",
    "@types/react-window": "^1.8.8",
    "@vitejs/plugin-react": "^4.7.0",
    "concurrently": "^9.2.1",
    "react-devtools": "^7.0.1",
    "tailwindcss": "^3.4.18",
    "typescript": "^5.6.3",
    "vite": "^5.4.21"
  }
}







